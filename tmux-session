## Default tmux-session

splitw -v -p 46 -t 0 								# split window vertically

splitw -h -p 88 -t 0 								# create top left column

splitw -h -p 50 -t 1 '~/bin/mynet'	# split the bottom horizontally and start iftop

splitw -v -p 68 -t 3 '~/bin/mylog' 	# split the bottom right vertically and start multitail

selectp -t 0												# return to the new Pane 0 in foreground
